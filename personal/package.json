  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/mapineda48/djs.git"
  },
  "main": "router",
  "dependencies": {
    "@mapineda48/util": "0.0.5",
    "@popperjs/core": "^2.9.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "clsx": "^1.1.1",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "mapineda-cra": "^0.0.0",
    "mapineda-react": "0.0.32",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "normalize.css": "^8.0.1",
    "puppeteer": "^8.0.0",
    "query-string": "^6.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.32.8",
    "smooth-scroll-into-view-if-needed": "^1.1.30",
    "socket.io": "^3.1.2",
    "socket.io-client": "^3.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "mapineda --start",
    "test": "react-scripts test",
    "check:types": "tsc && tsc -P router/tsconfig.json --noEmit",
    "build:view": "mapineda --build --url / -A",
    "build:model": "ts-node -P tsconfig.node.json tasks/model.ts",
    "postbuild:chat": "ts-node -P tsconfig.node.json tasks/chat.ts",
    "start:bin": "nodemon bin/index.ts",
    "build:lib": "tsc -P lib/tsconfig.json",
    "build:web": "tsc -P web/tsconfig.json",
    "tasks": "ts-node -P tsconfig.node.json tasks/index.ts",
    "render": "ts-node -P tsconfig.node.json tasks/render.ts",
    "build": "yarn build:view && yarn build:model && yarn postbuild:chat && yarn render && yarn build:router && yarn build:web && yarn tasks"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-scripts": {
    "personal": {
      "entry": "src/personal/index.tsx",
      "output": "build",
      "url": "./"
    },
    "admin": {
      "entry": "src/app/admin/index.tsx",
      "output": "build/admin",
      "url": "./"
    },
    "chat": {
      "entry": "src/app/chat/index.tsx",
      "output": "build/chat",
      "url": "./"
    },
    "default": "personal"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node -r dotenv/config -P tsconfig.node.json"
    }
  }
}
